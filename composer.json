{
  "name": "optimistdigital/nova-dependency-container",
  "description": "A Laravel Nova field container allowing to depend on other fields values",
  "keywords": [
    "laravel",
    "nova"
  ],
  "license": "MIT",
  "require": {
    "php": ">=8.0",
    "laravel/nova": "^4.0"
  },
  "autoload": {
    "psr-4": {
      "OptimistDigital\\NovaDependencyContainer\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "OptimistDigital\\NovaDependencyContainer\\FieldServiceProvider"
      ]
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "repositories": [
    {
      "type": "composer",
      "url": "https://nova.laravel.com"
    }
  ]
}
